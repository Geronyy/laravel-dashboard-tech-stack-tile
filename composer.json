{
    "name": "geronyy/laravel-dashboard-tech-stack-tile",
    "description": "A tech stack tile for the Laravel Dashboard",
    "keywords": [
        "quarterloop",
        "laravel-dashboard-tech-stack-tile"
    ],
    "homepage": "https://github.com/geronyy/laravel-dashboard-tech-stack-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Gero Wallisch",
            "email": "info@gerowallisch.com",
            "homepage": "gerowallisch.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Geronyy\\TechStackTile\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geronyy\\TechStackTile\\TechStackTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
